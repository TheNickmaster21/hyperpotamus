normalized: true
defaults: 
 request: null
 actions: 
  - status: 200 
steps: 
  - request: 
      url: "http://localhost:3000/static/test.html"
      headers: 
       user-agent: hyperpotamus
    actions:
      - text: This is a test
      - jquery: a
        count: 2
        capture:
          href: "@href"
          text: text
          innerHTML: innerHTML
          html: html
          outerHTML: outerHTML
          all_href: [ "@href" ]
          all_text: [ text ]
          all_innerHTML: [ innerHTML ]
          all_html: [ html ]
          all_outerHTML: [ outerHTML ]
      - compare: [ 'Link 1', <%= text %> ]
        operator: "="

