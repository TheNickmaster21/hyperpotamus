steps:
  - set:
      source:
        visited: 0
      mode: set
    path: $.steps.0
  - if:
      compare:
        operator: <
        array:
          - <%! visited %>
          - 3
      path: $.steps.1.if
    then:
      - set:
          source:
            visited: '<%! visited | plus,1 %>'
          mode: set
        path: $.steps.1.then.0
      - goto: SELF
        path: $.steps.1.then.1
    path: $.steps.1
  - compare:
      operator: =
      array:
        - <%! visited %>
        - 3
    path: $.steps.2
normalized: true
