steps:
  - actions:
      - merge:
          source:
            data:
              included: true
          override: true
        path: $.steps.0.actions.0
      - noop: true
        path: $.steps.0.actions.1
    path: $.steps.0
  - actions:
      - compare:
          operator: =
          array:
            - <%! data.included %>
            - true
        path: $.steps.1.actions.0
    path: $.steps.1
normalized: true
