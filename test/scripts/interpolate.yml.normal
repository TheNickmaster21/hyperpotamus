steps:
  - actions:
      merge:
        source:
          now: <% | now %>
        override: true
      path: $.steps.0.actions
    path: $.steps.0
  - actions:
      - request:
          url: 'http://localhost:3000/get?url=<% now | date_format,YYYYMMDD %>'
        response:
          regex:
            pattern: 'url":"<% now | date_format,YYYYMMDD %>'
            options: ''
          path: $.steps.1.actions.0.response
        path: $.steps.1.actions.0
    path: $.steps.1
normalized: true
