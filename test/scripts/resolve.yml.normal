steps:
  - actions:
      - set:
          source:
            base: 'http://example.com/one'
          mode: set
        path: $.steps.0.actions.0
      - set:
          source:
            answer: '<% ''/two'' | resolve_url,`base` %>'
          mode: set
        path: $.steps.0.actions.1
      - compare:
          operator: =
          array:
            - <% answer %>
            - 'http://example.com/two'
        path: $.steps.0.actions.2
    path: $.steps.0
normalized: true
