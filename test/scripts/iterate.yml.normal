steps:
  - actions:
      - set:
          source:
            target:
              - a
              - b
              - c
            result: ''
          mode: set
        path: $.steps.0.actions.0
    path: $.steps.0
  - actions:
      - set:
          source:
            result: <% result %><% target | current %>
          mode: set
        path: $.steps.1.actions.0
      - iterate:
          - target
        next:
          goto: SELF
          path: $.steps.1.actions.1.next
        path: $.steps.1.actions.1
    path: $.steps.1
  - actions:
      compare:
        operator: =
        array:
          - abc
          - <% result %>
      path: $.steps.2.actions
    path: $.steps.2
normalized: true
